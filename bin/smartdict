#!/usr/bin/env ruby

$LOAD_PATH.unshift File.expand_path('../../lib', __FILE__)
require 'rubygems'

require 'smartdict'
require 'smartdict/runner'

# TODO: remove before release
require 'pry'

Smartdict.env = :user

args = ARGV.clone

Smartdict.load_plugins

# TODO :move Smartdict.run to Runner.run
Smartdict.run
Smartdict::Runner.run(args)
